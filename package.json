{
  "name": "react-webpack-boilerplate",
  "version": "1.0.0",
  "description": "A quick boilerplate for setting up react with webpack and babel",
  "main": "index.js",
  "scripts": {
    "start": "gulp develop",
    "serve:develop": "webpack-dev-server --inline --open",
    "serve:production": "gulp package && node ./scripts/serve-package",
    "build:develop": "webpack",
    "build:production": "cross-env NODE_ENV=production webpack --env.production -p",
    "package": "gulp package"
  },
  "repository": "https://github.com/chris-pikul/react-webpack-boilerplate.git",
  "author": "Chris Pikul <chris@workmaple.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.3",
    "@babel/plugin-proposal-optional-chaining": "^7.10.3",
    "@babel/plugin-proposal-private-methods": "^7.10.1",
    "@babel/plugin-proposal-private-property-in-object": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "autoprefixer": "^9.8.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "del": "^5.1.0",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "nunjucks": "^3.2.1",
    "nunjucks-webpack-plugin": "^5.0.0",
    "postcss": "^7.0.32",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "details": {
    "title": "React Webpack Boilerplate",
    "charset": "utf-8",
    "meta": {
      "author": "Chris Pikul",
      "description": "A simple React boilerplate, using Webpack, BabelJS, and SASS/PostCSS"
    },
    "opengraph": {
      "og:title": "React Webpack Boilerplate",
      "og:description": "A simple React boilerplate, using Webpack, BabelJS, and SASS/PostCSS"
    }
  }
}
